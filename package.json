{
  "name": "tg-bot-dashboard",
  "version": "1.0.0",
  "description": "Admin dashboard for Telegram bot data management",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx watch src/server/index.ts",
    "dev:client": "cd src/client && vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd src/client && vite build",
    "build:server": "tsx src/server/index.ts",
    "preview": "cd src/client && vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src"
  },
  "keywords": ["telegram", "bot", "dashboard", "admin"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hono/node-server": "^1.13.7",
    "@supabase/supabase-js": "^2.46.1",
    "hono": "^4.6.10",
    "dotenv": "^16.4.5",
    "axios": "^1.7.7",
    "naive-ui": "^2.40.1",
    "pinia": "^2.2.6",
    "vue": "^3.5.13",
    "vue-router": "^4.4.5",
    "@vicons/ionicons5": "^0.12.0"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "@vitejs/plugin-vue": "^5.2.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.15.0",
    "eslint-plugin-vue": "^9.31.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.1"
  }
}